{
  "name": "leapfrog",
  "version": "1.18.0",
  "private": true,
  "scripts": {
    "react-native": "react-native",
    "ci:clean": "cd ios && rm -rf Pods/ && rm -rf ~/Library/Developer/Xcode/DerivedData && rm -rf ~/Library/Developer/Xcode/Archives && git clean -dfx && pod deintegrate && pod update && cd ..",
    "build:android:dev": "yarn env:dev && cd android && ./gradlew assembleDevRelease",
    "build:android:uat": "yarn env:uat && cd android && ./gradlew assembleUatRelease",
    "build:android:prod": "yarn env:prod && cd android && ./gradlew assembleProdRelease",
    "android": "react-native run-android",
    "android:dev": "yarn android --variant=devDebug --appIdSuffix dev",
    "android:qa": "yarn android --variant=qaDebug --appIdSuffix qa",
    "android:uat": "yarn android --variant=uatDebug --appIdSuffix uat",
    "android:prod": "yarn android --variant=prodDebug",
    "ios": "npx react-native run-ios",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "clean:ios": "cd ios && xcodebuild clean && cd ../",
    "clean": "yarn clean:android && yarn clean:ios",
    "start": "cross-env environment=development node ./scripts/build-index.js && react-native start --reset-cache  | tee ./logs/$(date '+%Y_%m_%d__%H_%M_%S').log",
    "start:android:dev": "yarn clean:android && yarn env:dev && concurrently \"yarn start\" \"yarn android:dev\"",
    "start:android:qa": "yarn clean:android && yarn env:qa && concurrently \"yarn start\" \"yarn android:qa\"",
    "start:android:uat": "yarn clean:android && yarn env:uat && concurrently \"yarn start\" \"yarn android:uat\"",
    "start:android:prod": "yarn clean:android && yarn env:prod && concurrently \"yarn start\" \"yarn android:prod\"",
    "start:ios:dev": "yarn clean:ios && yarn env:dev && concurrently \"yarn start\" \"yarn ios --configuration=Debug\"",
    "start:ios:qa": "yarn clean:ios && yarn env:qa && concurrently \"yarn start\" \"yarn ios --configuration=QaDebug\"",
    "start:ios:uat": "yarn clean:ios && yarn env:uat && concurrently \"yarn start\" \"yarn ios --configuration=UatDebug\"",
    "start:ios:prod": "yarn clean:ios && yarn env:prod && concurrently \"yarn start\" \"yarn ios --configuration=Release\"",
    "start:all": "yarn clean && yarn env:dev && concurrently \"yarn start\" \"yarn ios\" \"yarn android:dev\"",
    "storybook": "yarn env:dev && cross-env environment=storybook node ./scripts/build-index.js && react-native start",
    "storybook:android": "yarn clean:android && concurrently \"yarn storybook\" \"yarn android:dev\"",
    "storybook:ios": "yarn clean:ios && concurrently \"yarn storybook\" \"yarn ios\"",
    "test": "jest --clear-cache && yarn env:test && cross-env TZ=America/Los_Angeles jest --watchAll=false --coverage",
    "test:prepush": "yarn ts:check && yarn lint && yarn prettier:check && yarn spellcheck && yarn test",
    "test:ci": "yarn report-versions && yarn install && yarn test:prepush && yarn ci:clean",
    "lint": "eslint . --ext .ts,.tsx",
    "prettier": "prettier --parser typescript --single-quote",
    "prettier:check": "yarn prettier --list-different \"(src|storybook|scripts)/**/*.{ts,tsx,js}\"",
    "format": "yarn prettier --write \"(src|storybook|scripts)/**/*.{ts,tsx,js}\"",
    "env:test": "node ./scripts/set-env.js test",
    "env:dev": "node ./scripts/set-env.js dev",
    "env:qa": "node ./scripts/set-env.js qa",
    "env:uat": "node ./scripts/set-env.js uat",
    "env:prod": "node ./scripts/set-env.js prod",
    "ts:check": "tsc --project ./tsconfig.json --noEmit --skipLibCheck",
    "bump:major": "react-native bump-version --type major --skip-code-for all",
    "bump:minor": "react-native bump-version --type minor --skip-code-for all",
    "bump:patch": "react-native bump-version --type patch --skip-code-for all",
    "report-versions": "node ./scripts/report-versions.js",
    "generate-licenses": "yarn licenses list --prod --json --no-progress | ./node_modules/node-jq/bin/jq --raw-output --from-file license-generator.jq > EXTERNAL-LICENSES.md",
    "postinstall": "patch-package && node ./scripts/prepare-android-build.js",
    "spellcheck": "cspell --no-progress --relative \"src/**/*.{ts,tsx,js}\"",
    "uri-scheme": "npx uri-scheme",
    "open-scheme": "yarn uri-scheme open",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/native": "^11.0.0-next.18",
    "@emotion/react": "^11.9.0",
    "@react-native-community/art": "^1.2.0",
    "@react-native-community/clipboard": "^1.5.1",
    "@react-native-community/datetimepicker": "^3.5.2",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^5.9.10",
    "@react-native-community/picker": "^1.8.1",
    "@react-native-cookies/cookies": "^6.2.1",
    "@react-native-firebase/analytics": "^12.6.1",
    "@react-native-firebase/app": "^12.6.1",
    "@react-native-firebase/crashlytics": "^12.6.1",
    "@react-native-firebase/messaging": "^12.6.1",
    "@react-native-firebase/perf": "^12.6.1",
    "@react-native-firebase/remote-config": "^12.6.1",
    "@react-navigation/bottom-tabs": "^5.11.15",
    "@react-navigation/material-top-tabs": "^5.3.19",
    "@react-navigation/native": "^5.9.8",
    "@react-navigation/stack": "^5.14.9",
    "jest": "^27.5.1",
    "lottie-react-native": "^5.1.3",
    "moment": "^2.29.3",
    "npm": "^7.22.0",
    "patch-package": "^6.4.7",
    "path": "^0.12.7",
    "postinstall-postinstall": "^2.1.0",
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-native-advertising-id": "^1.0.11",
    "react-native-animatable": "^1.3.3",
    "react-native-appsflyer": "^6.5.21",
    "react-native-auth0": "^2.13.1",
    "react-native-barcode-builder": "^2.0.0",
    "react-native-base64": "^0.2.1",
    "react-native-color-matrix-image-filters": "^5.2.14",
    "react-native-device-info": "^8.7.1",
    "react-native-dotenv": "^3.2.0",
    "react-native-fast-image": "^8.5.11",
    "react-native-forter": "https://forter-mobile-git:zvGKcVtDhkfj4asNekSn@bitbucket.org/forter-mobile/forter-react-plugin.git#v0.1.13",
    "react-native-fs": "^2.20.0",
    "react-native-geolocation-service": "^5.3.0-beta.4",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-json-tree": "^1.3.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localize": "^2.2.1",
    "react-native-maps": "^0.29.3",
    "react-native-modal-datetime-picker": "^11.0.0",
    "react-native-onetrust-cmp": "6.35.0",
    "react-native-permissions": "^3.3.1",
    "react-native-progress": "^5.0.0",
    "react-native-reanimated": "^2.8.0",
    "react-native-restart": "^0.0.22",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "3.6.0",
    "react-native-scroll-bottom-sheet": "^0.7.0",
    "react-native-secure-key-store": "^2.0.9",
    "react-native-sha256": "^1.4.7",
    "react-native-share": "^7.4.1",
    "react-native-splash-screen": "^3.3.0",
    "react-native-stars": "^1.2.2",
    "react-native-svg": "^12.3.0",
    "react-native-tab-view": "^2.16.0",
    "react-native-tracking-transparency": "^0.1.1",
    "react-native-vector-icons": "^9.1.0",
    "react-native-webview": "^11.18.2",
    "sanitize-html": "^2.7.0",
    "tealium-react-native": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/runtime": "^7.15.3",
    "@react-native-community/eslint-config": "^3.0.2",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/addons": "^6.4.22",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/react-native": "^9.1.0",
    "@types/jest": "^27.5.1",
    "@types/moment": "^2.13.0",
    "@types/npm": "^7.19.0",
    "@types/react-native": "^0.65.1",
    "@types/react-native-appsflyer": "^5.1.2",
    "@types/react-native-auth0": "^2.13.0",
    "@types/react-native-base64": "^0.1.0",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-native-fs": "^2.13.0",
    "@types/react-native-linear-gradient": "^2.4.0",
    "@types/react-native-share": "^3.3.3",
    "@types/react-test-renderer": "17.0.1",
    "@types/sanitize-html": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.30.0",
    "babel-jest": "^27.1.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "cspell": "^5.21.0",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^26.2.2",
    "husky": "^7.0.0",
    "metro-react-native-babel-preset": "^0.67.0",
    "node-jq": "^2.3.3",
    "prettier": "^2.6.2",
    "react-dom": "17.0.2",
    "react-hooks-testing-library": "^0.6.0",
    "react-native-cli-bump-version": "^1.4.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.3.5",
    "uri-scheme": "^1.0.120"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{ts,tsx,js}",
      "!<rootDir>/src/test-utils/**/*.ts",
      "!<rootDir>/src/**/*.test.{ts,tsx,js}",
      "!<rootDir>/src/**/styles.ts",
      "!<rootDir>/src/**/index.tsx",
      "!<rootDir>/src/**/index.ts",
      "!<rootDir>/src/constants/*.ts",
      "!<rootDir>/src/models/*.ts",
      "!<rootDir>/src/commons/models/*.ts",
      "!<rootDir>/src/AppRoot.tsx",
      "!<rootDir>/src/state-mgmt/types.ts",
      "!<rootDir>/src/state-mgmt/dependencies.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coverageReporters": [
      "lcov",
      "text",
      "text-summary"
    ],
    "reporters": [
      "default"
    ],
    "setupFilesAfterEnv": [
      "./jestSetup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|@react-native-community|@react-navigation|react-native-color-matrix-image-filters|rn-color-matrices|concat-color-matrices|react-native-barcode-builder|jsbarcode|tealium-react-native)"
    ]
  }
}
